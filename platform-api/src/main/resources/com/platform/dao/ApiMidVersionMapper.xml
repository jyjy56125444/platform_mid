<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.dao.ApiMidVersionMapper">

    <select id="queryObject" resultType="com.platform.entity.MidVersionVo">
        select * from mobile_version where app_id = #{appId} and version_type = #{appType}
    </select>
</mapper>